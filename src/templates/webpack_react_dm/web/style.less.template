.<%=name%> {
  padding: 1rem;
  text-align: center;
  .title {
    margin: 0;
    color: #00b3ee;
  }
  .btn {
    border: 0;
    display: inline-block;
    vertical-align: top;
    cursor: pointer;
    text-align: center;
    height: 44px;
    line-height: 44px;
    font-size: 16px;
    padding: 0 16px;
    border-radius: 4px;
    background-color: #ff6633;
    color: #ffffff;
    outline: none;
  }
}